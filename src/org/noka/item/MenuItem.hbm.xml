<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.noka.item">
<class name="MenuItem" table="NK_SYS_MENU">
<id name="menuid">
	<generator class="increment"/>
</id>
<property name="menupid" />
<property name="menuname" />
<property name="menuurl" />
<property name="menuoperate" />
<property name="menuimage" />
<property name="menutaxis" />
<property name="menutarget" />
<property name="menunburl" />
<property name="menuleve" />
</class>
</hibernate-mapping>
